<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#"
     xml:base="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:baco-spanish-wine-ontology="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology">
        <rdfs:comment xml:lang="en">BACO Spanish Wine Ontology, by David Delgado Camacho and Pablo Pimàs Verge based on the  Kagle: Spanish Wine Quality Dataset from Fedesoriano.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/licenseDocument -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/licenseDocument"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->


    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#license -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWine">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>Inverse relationship of producedIn. A Region allocates different Wines</rdfs:comment>
        <rdfs:label>Allocate wine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWinery -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWinery">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producesIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
        <rdfs:comment>Inverse relationship of producesIn. A Region allocates a Winery</rdfs:comment>
        <rdfs:label>Allocate winery</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasVariety -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasVariety">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#varietyHasWine"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety"/>
        <rdfs:comment xml:lang="en">This object property refers to the specific grape variety or varieties used in the production of a wine, linking a wine instance to one or more grape varieties that were used in its production.</rdfs:comment>
        <rdfs:label>Has variety</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWine">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWineType"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>Inverse relationship of hasWineType</rdfs:comment>
        <rdfs:label>Has wine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWineType -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWineType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType"/>
        <rdfs:comment xml:lang="en">This object property is used in the context of wines to describe the type or style of a wine. It links a wine to its corresponding wine type, such as red, white, or rosé. It is a useful property for categorizing and searching for wines based on their style or type.</rdfs:comment>
        <rdfs:label>Has wine type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#isPairedWith -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#isPairedWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#pairsWith"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>Inverse relationship of pairsWith</rdfs:comment>
        <rdfs:label>Is paired with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#makes -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#makes">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>Inverse realtionship of producedBy</rdfs:comment>
        <rdfs:label>Makes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#pairsWith -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#pairsWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
        <rdfs:comment xml:lang="en">This is an object property that relates a food item to a wine that is known to complement its flavor. It can be used to suggest a suitable wine pairing based on the characteristics of the food, such as its ingredients, texture, and preparation method.</rdfs:comment>
        <rdfs:label>Pairs with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
        <rdfs:comment xml:lang="en">This object property indicates the winery that produces a particular wine. It relates a wine to a winery, where the winery is the subject and the wine is the object.</rdfs:comment>
        <rdfs:label>Produced by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:comment xml:lang="en">This object property represents the relationship between a winery and a geographic location where the wine is produced. This property is used to link a winery with the wine regions where its wines are made.</rdfs:comment>
        <rdfs:label>Produced in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producesIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producesIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:comment xml:lang="en">This object property is used to describe the relationship between a winery and a region where the winery produces wine. It links the Winery class to the Region class, indicating the region where the wine is produced.</rdfs:comment>
        <rdfs:label>Produces in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#varietyHasWine -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#varietyHasWine">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>Inverse relationship of hasWineType</rdfs:comment>
        <rdfs:label>Variety has wine</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#entityName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#entityName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Any entity can be expressed by its name</rdfs:comment>
        <rdfs:label>Entity name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#foodName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#foodName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the name of the food. There can be many of theese names for one entity.</rdfs:comment>
        <rdfs:label>Food name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#grapeVarietyName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#grapeVarietyName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of a grape variety that acts as an ingredient of a Wine entity</rdfs:comment>
        <rdfs:label>Grape variety name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#regionName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#regionName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the Region where Wine is produced</rdfs:comment>
        <rdfs:label>Region name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#typeName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#typeName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the type of Wine produced (Red, White, Sparkling, Fortified... )</rdfs:comment>
        <rdfs:label>Type name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineAcidity -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineAcidity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Acidity score, defined as wine&apos;s “pucker” or tartness; it&apos;s what makes a wine refreshing and your tongue salivate and want another sip [from 1-5]</rdfs:comment>
        <rdfs:label>Wine acidity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineBody -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineBody">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Body score, defined as the richness and weight of the wine in your mouth [from 1-5]</rdfs:comment>
        <rdfs:label>Wine body</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the product made by the winery</rdfs:comment>
        <rdfs:label>Wine name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#winePrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#winePrice">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Represents the Wine price in EUR</rdfs:comment>
        <rdfs:label>Wine price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineRating">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0</xsd:minExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Represents the average rating given to the wine by the users [from 1-5]</rdfs:comment>
        <rdfs:label>Wine rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineReviews -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineReviews">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Represents the number of users that reviewed the wine</rdfs:comment>
        <rdfs:label>Wine reviews</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineVintage -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineVintage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Year in which the grapes were harvested</rdfs:comment>
        <rdfs:label>Wine vintage</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineryName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineryName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Winery name that produces the Wine</rdfs:comment>
        <rdfs:label>Winery name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Acidity -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Acidity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment xml:lang="en">It refers to the perceived acidity in a wine, which is typically expressed on a scale from low to high or as a specific pH value. It also specifies that acidity is measured by the amount of total titratable acidity, usually expressed in grams per litre of wine.</rdfs:comment>
        <rdfs:label>Acidity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment xml:lang="en">The wine body refers to the perceived weight or fullness of a wine in the mouth, which is influenced by factors such as grape variety, alcohol content, and ageing conditions. It also specifies that wine bodies can range from light-bodied to full-bodied wines.</rdfs:comment>
        <rdfs:label>Body</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#CompleteBodyWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#CompleteBodyWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LightBodyWine"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#MediumBodyWine"/>
        <rdfs:comment xml:lang="en">A full-bodied wine has a rich and robust taste, with a higher alcohol content and more intense flavors. These wines have a heavier texture and mouthfeel, making them perfect for pairing with bold and hearty dishes such as steak or roasted vegetables.</rdfs:comment>
        <rdfs:label>Complete body wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Economic -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Economic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Expensive"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pricy"/>
        <rdfs:comment xml:lang="en">Wines with price less than 60</rdfs:comment>
        <rdfs:label>Economic</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Expensive -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Expensive">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pricy"/>
        <rdfs:comment xml:lang="en">Wines with price starting from 300</rdfs:comment>
        <rdfs:label>Expensive</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#ExpensiveWineRegion -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#ExpensiveWineRegion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
        <rdfs:comment>An exclusive wine region producing high-quality wines with prices typically above average, often due to its unique geographical or climatic characteristics.</rdfs:comment>
        <rdfs:label>Expensive wine region</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Fish -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Fish">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#SeaFood"/>
        <rdfs:comment xml:lang="en">Fish is a class of aquatic creatures that are commonly used as food. They typically have a mild flavor and tender texture, making them a popular choice for cooking. There are many different species of fish that can be used in a variety of dishes, such as sushi, fish and chips, and grilled fish.</rdfs:comment>
        <rdfs:label>Fish</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#foodName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:comment xml:lang="en">Food items that can be paired with wines to enhance the tasting experience. It also explains that the process of food pairing with wine involves selecting a dish that complements or contrasts the wine&apos;s flavour, aroma, and body</rdfs:comment>
        <rdfs:label>Food</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#grapeVarietyName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:comment xml:lang="en">A grape variety represents a specific type of grape used in the production of wine. Grape varieties have distinct characteristics, such as flavour, colour, and aroma, that influence the taste and style of the wine. Different grape varieties are better suited to specific climates and soil types, which can affect their growth and quality.</rdfs:comment>
        <rdfs:label>Grape Variety</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighAcidityWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighAcidityWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Acidity"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LowAcidityWine"/>
        <rdfs:comment xml:lang="en">Wines with acidity higher than 3</rdfs:comment>
        <rdfs:label>High acidity wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighRatedWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighRatedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Rating"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#VeryHighRatedWine"/>
        <rdfs:comment xml:lang="en">High rated wine refers to a wine that has received a high score or rating from wine critics or experts based on various factors such as taste, aroma, body, and complexity.</rdfs:comment>
        <rdfs:label>High rated wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LightBodyWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LightBodyWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#MediumBodyWine"/>
        <rdfs:comment xml:lang="en">A light body wine is characterized by its lower alcohol content, mild taste, and refreshing finish. Typically paired with lighter dishes, such as salads, seafood, and poultry, it provides a perfect complement to meals without overwhelming the palate.&quot;</rdfs:comment>
        <rdfs:label>Light body wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LowAcidityWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LowAcidityWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Acidity"/>
        <rdfs:comment xml:lang="en">Wines with acidity lower or equal to 3</rdfs:comment>
        <rdfs:label>Low acidity wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#MediumBodyWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#MediumBodyWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body"/>
        <rdfs:comment xml:lang="en">Medium body wine is a type of wine that falls in between light and full-bodied wines. It typically has a moderate alcohol content and a balance of flavor and texture, making it a versatile wine that can pair well with a range of foods.</rdfs:comment>
        <rdfs:label>medium body wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#NumberOfReviews -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#NumberOfReviews">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>The total number of reviews given to a wine by critics or industry specialists.</rdfs:comment>
        <rdfs:label>Number of reviews</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#OverTenThousendReviewsWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#OverTenThousendReviewsWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#NumberOfReviews"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#UnderFortyReviewsWine"/>
        <rdfs:comment>Wines with more than 10000 reviews</rdfs:comment>
        <rdfs:label>Over ten thousand reviews wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pairing -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pairing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment xml:lang="en">Pairing is the art of combining food and wine to enhance the flavors and create a harmonious dining experience. The right pairing can elevate a dish to a whole new level and make the wine taste even better. It&apos;s all about finding the perfect balance between the food and the wine to create a truly memorable meal.</rdfs:comment>
        <rdfs:label>Pairing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment xml:lang="en">The wine price represents the cost of a bottle of wine in a particular currency. Wine prices can vary widely depending on factors such as grape variety, winemaking techniques, ageing, and the reputation of the producer or region. Wine prices can also be influenced by external factors such as supply and demand, economic conditions, and exchange rates.</rdfs:comment>
        <rdfs:label>Price</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pricy -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pricy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price"/>
        <rdfs:comment xml:lang="en">Wines with price equals to 60 but less than 300</rdfs:comment>
        <rdfs:label>Pricy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Rating -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Rating">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine"/>
        <rdfs:comment>This property is used to indicate the quality or rating of a particular wine, usually on a scale from 0 to 5. This rating can be given by various wine critics or publications and is used as a guide to help consumers make informed choices when purchasing wine.</rdfs:comment>
        <rdfs:label>Rating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#RedMeat -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#RedMeat">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#SeaFood"/>
        <rdfs:comment xml:lang="en">Red meat is a food category that includes various types of meat, such as beef, lamb, and pork, which are typically red or dark in color when raw. These meats are often considered rich in protein and iron, and are commonly paired with full-bodied red wines to complement their flavors.</rdfs:comment>
        <rdfs:label>Red meat</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#regionName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:comment xml:lang="en">A WineRegion represents a specific geographical area known for wine production, characterised by its unique climate, soil, and topography. Each region may have distinct regulations and wine styles, and it can contain multiple subregions and wineries.</rdfs:comment>
        <rdfs:label>Region</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Salmon -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Salmon">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Fish"/>
        <rdfs:comment xml:lang="en">Salmon is a type of fish that is native to the North Atlantic and Pacific Oceans and is known for its delicate flavor, rich texture, and versatility in cooking. It is commonly grilled, baked, or smoked, and is a popular ingredient in salads, sushi, and other dishes. Salmon is also a good source of omega-3 fatty acids, protein, and other nutrients.</rdfs:comment>
        <rdfs:label>Salmon</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#SeaFood -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#SeaFood">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
        <rdfs:comment xml:lang="en">Seafood refers to a wide range of edible sea creatures, including fish, shellfish, and other marine animals. These dishes are often enjoyed for their unique flavors and health benefits, such as being low in fat and high in protein. From grilled fish to seafood paella, there are many delicious and nutritious ways to enjoy this culinary category.</rdfs:comment>
        <rdfs:label>Sea food</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#UnderFortyReviewsWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#UnderFortyReviewsWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#NumberOfReviews"/>
        <rdfs:comment>Wines with less than 40 reviews</rdfs:comment>
        <rdfs:label>Under forty reviews wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#VeryHighRatedWine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#VeryHighRatedWine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Rating"/>
        <rdfs:comment>Medium rated wine refers to a wine that has received a score or rating in the middle of the rating scale, usually indicating a good but not exceptional quality. It may be a versatile choice for pairing with different types of food or enjoyed on its own.</rdfs:comment>
        <rdfs:label>Very high rated wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasVariety"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#pairsWith"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWineType"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Wine is an alcoholic beverage made from fermented grapes. It can vary greatly in flavor, color, and texture depending on factors such as grape variety, region, and winemaking techniques. Wines are commonly classified into categories such as red, white, rosé, and sparkling based on their color and carbonation level.</rdfs:comment>
        <rdfs:label>Wine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#entityName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">The root class representing the most general concept related to the wine domain.</rdfs:comment>
        <rdfs:label>Wine entity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WinePairedWithRedMeat -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WinePairedWithRedMeat">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pairing"/>
        <rdfs:comment>Wine that can be paired with red meat</rdfs:comment>
        <rdfs:label>Wine paired with red meat</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WinePairedWithSalmon -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WinePairedWithSalmon">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pairing"/>
        <rdfs:comment xml:lang="en">Salmon pairs well with a variety of wines, but typically lighter-bodied white wines or medium-bodied red wines such as Pinot Noir. The flavors of the wine should complement the flavors of the salmon, creating a balanced and enjoyable pairing experience.</rdfs:comment>
        <rdfs:label>Wine paired with salmon</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineType">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#typeName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity"/>
        <rdfs:comment xml:lang="en">Wine type represents the style or category of a wine based on its characteristics, such as flavour, colour, and aroma. Wine types can vary widely depending on factors such as the grape varieties used, the region where the wine is produced, and the winemaking techniques employed.</rdfs:comment>
        <rdfs:label>Wine type</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Winery">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineryName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
