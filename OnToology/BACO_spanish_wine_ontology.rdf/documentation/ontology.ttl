@prefix : <http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#> .

<http://www.semanticweb.org/ontology/baco-spanish-wine-ontology> rdf:type owl:Ontology ;
                                                                  rdfs:comment "BACO Spanish Wine Ontology, by David Delgado Camacho and Pablo Pimàs Verge based on the  Kagle: Spanish Wine Quality Dataset from Fedesoriano."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/licenseDocument
<http://purl.org/dc/terms/licenseDocument> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#license
rdf:license rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWine
:allocateWine rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :producedIn ;
              rdfs:domain :Region ;
              rdfs:range :Wine ;
              rdfs:comment "Inverse relationship of producedIn. A Region allocates different Wines" ;
              rdfs:label "Allocate wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#allocateWinery
:allocateWinery rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :producesIn ;
                rdfs:domain :Region ;
                rdfs:range :Winery ;
                rdfs:comment "Inverse relationship of producesIn. A Region allocates a Winery" ;
                rdfs:label "Allocate winery" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasVariety
:hasVariety rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :varietyHasWine ;
            rdfs:domain :Wine ;
            rdfs:range :GrapeVariety ;
            rdfs:comment "This object property refers to the specific grape variety or varieties used in the production of a wine, linking a wine instance to one or more grape varieties that were used in its production."@en ;
            rdfs:label "Has variety" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWine
:hasWine rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :hasWineType ;
         rdfs:domain :WineType ;
         rdfs:range :Wine ;
         rdfs:comment "Inverse relationship of hasWineType" ;
         rdfs:label "Has wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#hasWineType
:hasWineType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Wine ;
             rdfs:range :WineType ;
             rdfs:comment "This object property is used in the context of wines to describe the type or style of a wine. It links a wine to its corresponding wine type, such as red, white, or rosé. It is a useful property for categorizing and searching for wines based on their style or type."@en ;
             rdfs:label "Has wine type" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#isPairedWith
:isPairedWith rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :pairsWith ;
              rdfs:domain :Food ;
              rdfs:range :Wine ;
              rdfs:comment "Inverse relationship of pairsWith" ;
              rdfs:label "Is paired with" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#makes
:makes rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       owl:inverseOf :producedBy ;
       rdfs:domain :Winery ;
       rdfs:range :Wine ;
       rdfs:comment "Inverse realtionship of producedBy" ;
       rdfs:label "Makes" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#pairsWith
:pairsWith rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Wine ;
           rdfs:range :Food ;
           rdfs:comment "This is an object property that relates a food item to a wine that is known to complement its flavor. It can be used to suggest a suitable wine pairing based on the characteristics of the food, such as its ingredients, texture, and preparation method."@en ;
           rdfs:label "Pairs with" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Wine ;
            rdfs:range :Winery ;
            rdfs:comment "This object property indicates the winery that produces a particular wine. It relates a wine to a winery, where the winery is the subject and the wine is the object."@en ;
            rdfs:label "Produced by" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producedIn
:producedIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Wine ;
            rdfs:range :Region ;
            rdfs:comment "This object property represents the relationship between a winery and a geographic location where the wine is produced. This property is used to link a winery with the wine regions where its wines are made."@en ;
            rdfs:label "Produced in" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#producesIn
:producesIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Winery ;
            rdfs:range :Region ;
            rdfs:comment "This object property is used to describe the relationship between a winery and a region where the winery produces wine. It links the Winery class to the Region class, indicating the region where the wine is produced."@en ;
            rdfs:label "Produces in" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#varietyHasWine
:varietyHasWine rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :GrapeVariety ;
                rdfs:range :Wine ;
                rdfs:comment "Inverse relationship of hasWineType" ;
                rdfs:label "Variety has wine" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#entityName
:entityName rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :WineEntity ;
            rdfs:range xsd:string ;
            rdfs:comment "Any entity can be expressed by its name" ;
            rdfs:label "Entity name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#foodName
:foodName rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Food ;
          rdfs:range xsd:string ;
          rdfs:comment "Represents the name of the food. There can be many of theese names for one entity." ;
          rdfs:label "Food name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#grapeVarietyName
:grapeVarietyName rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :GrapeVariety ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The name of a grape variety that acts as an ingredient of a Wine entity" ;
                  rdfs:label "Grape variety name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#regionName
:regionName rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Region ;
            rdfs:range xsd:string ;
            rdfs:comment "Name of the Region where Wine is produced" ;
            rdfs:label "Region name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#typeName
:typeName rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :WineType ;
          rdfs:range xsd:string ;
          rdfs:comment "Name of the type of Wine produced (Red, White, Sparkling, Fortified... )" ;
          rdfs:label "Type name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineAcidity
:wineAcidity rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Wine ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:integer ;
                          owl:withRestrictions ( [ xsd:minExclusive 0
                                                 ]
                                                 [ xsd:maxInclusive 5
                                                 ]
                                               )
                        ] ;
             rdfs:comment "Acidity score, defined as wine's “pucker” or tartness; it's what makes a wine refreshing and your tongue salivate and want another sip [from 1-5]" ;
             rdfs:label "Wine acidity" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineBody
:wineBody rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Wine ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:onDatatype xsd:integer ;
                       owl:withRestrictions ( [ xsd:minExclusive 0
                                              ]
                                              [ xsd:maxInclusive 5
                                              ]
                                            )
                     ] ;
          rdfs:comment "Body score, defined as the richness and weight of the wine in your mouth [from 1-5]" ;
          rdfs:label "Wine body" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineName
:wineName rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Wine ;
          rdfs:range xsd:string ;
          rdfs:comment "The name of the product made by the winery" ;
          rdfs:label "Wine name" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#winePrice
:winePrice rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :Wine ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Represents the Wine price in EUR" ;
           rdfs:label "Wine price" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineRating
:wineRating rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Wine ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:onDatatype xsd:decimal ;
                         owl:withRestrictions ( [ xsd:minExclusive 0
                                                ]
                                                [ xsd:maxInclusive 5
                                                ]
                                              )
                       ] ;
            rdfs:comment "Represents the average rating given to the wine by the users [from 1-5]" ;
            rdfs:label "Wine rating" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineReviews
:wineReviews rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Wine ;
             rdfs:range xsd:integer ;
             rdfs:comment "Represents the number of users that reviewed the wine" ;
             rdfs:label "Wine reviews" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineVintage
:wineVintage rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Wine ;
             rdfs:range xsd:integer ;
             rdfs:comment "Year in which the grapes were harvested" ;
             rdfs:label "Wine vintage" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#wineryName
:wineryName rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Winery ;
            rdfs:range xsd:string ;
            rdfs:comment "Winery name that produces the Wine" ;
            rdfs:label "Winery name" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Acidity
:Acidity rdf:type owl:Class ;
         rdfs:subClassOf :Wine ;
         rdfs:comment "It refers to the perceived acidity in a wine, which is typically expressed on a scale from low to high or as a specific pH value. It also specifies that acidity is measured by the amount of total titratable acidity, usually expressed in grams per litre of wine."@en ;
         rdfs:label "Acidity" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Body
:Body rdf:type owl:Class ;
      rdfs:subClassOf :Wine ;
      rdfs:comment "The wine body refers to the perceived weight or fullness of a wine in the mouth, which is influenced by factors such as grape variety, alcohol content, and ageing conditions. It also specifies that wine bodies can range from light-bodied to full-bodied wines."@en ;
      rdfs:label "Body" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#CompleteBodyWine
:CompleteBodyWine rdf:type owl:Class ;
                  rdfs:subClassOf :Body ;
                  owl:disjointWith :LightBodyWine ,
                                   :MediumBodyWine ;
                  rdfs:comment "A full-bodied wine has a rich and robust taste, with a higher alcohol content and more intense flavors. These wines have a heavier texture and mouthfeel, making them perfect for pairing with bold and hearty dishes such as steak or roasted vegetables."@en ;
                  rdfs:label "Complete body wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Economic
:Economic rdf:type owl:Class ;
          rdfs:subClassOf :Price ;
          owl:disjointWith :Expensive ,
                           :Pricy ;
          rdfs:comment "Wines with price less than 60"@en ;
          rdfs:label "Economic" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Expensive
:Expensive rdf:type owl:Class ;
           rdfs:subClassOf :Price ;
           owl:disjointWith :Pricy ;
           rdfs:comment "Wines with price starting from 300"@en ;
           rdfs:label "Expensive" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#ExpensiveWineRegion
:ExpensiveWineRegion rdf:type owl:Class ;
                     rdfs:subClassOf :Region ;
                     rdfs:comment "An exclusive wine region producing high-quality wines with prices typically above average, often due to its unique geographical or climatic characteristics." ;
                     rdfs:label "Expensive wine region" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Fish
:Fish rdf:type owl:Class ;
      rdfs:subClassOf :SeaFood ;
      rdfs:comment "Fish is a class of aquatic creatures that are commonly used as food. They typically have a mild flavor and tender texture, making them a popular choice for cooking. There are many different species of fish that can be used in a variety of dishes, such as sushi, fish and chips, and grilled fish."@en ;
      rdfs:label "Fish" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Food
:Food rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :foodName ;
                            owl:someValuesFrom xsd:string
                          ] ;
      rdfs:subClassOf :WineEntity ;
      rdfs:comment "Food items that can be paired with wines to enhance the tasting experience. It also explains that the process of food pairing with wine involves selecting a dish that complements or contrasts the wine's flavour, aroma, and body"@en ;
      rdfs:label "Food" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#GrapeVariety
:GrapeVariety rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :grapeVarietyName ;
                                    owl:someValuesFrom xsd:string
                                  ] ;
              rdfs:subClassOf :WineEntity ;
              rdfs:comment "A grape variety represents a specific type of grape used in the production of wine. Grape varieties have distinct characteristics, such as flavour, colour, and aroma, that influence the taste and style of the wine. Different grape varieties are better suited to specific climates and soil types, which can affect their growth and quality."@en ;
              rdfs:label "Grape Variety" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighAcidityWine
:HighAcidityWine rdf:type owl:Class ;
                 rdfs:subClassOf :Acidity ;
                 owl:disjointWith :LowAcidityWine ;
                 rdfs:comment "Wines with acidity higher than 3"@en ;
                 rdfs:label "High acidity wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#HighRatedWine
:HighRatedWine rdf:type owl:Class ;
               rdfs:subClassOf :Rating ;
               owl:disjointWith :VeryHighRatedWine ;
               rdfs:comment "High rated wine refers to a wine that has received a high score or rating from wine critics or experts based on various factors such as taste, aroma, body, and complexity."@en ;
               rdfs:label "High rated wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LightBodyWine
:LightBodyWine rdf:type owl:Class ;
               rdfs:subClassOf :Body ;
               owl:disjointWith :MediumBodyWine ;
               rdfs:comment "A light body wine is characterized by its lower alcohol content, mild taste, and refreshing finish. Typically paired with lighter dishes, such as salads, seafood, and poultry, it provides a perfect complement to meals without overwhelming the palate.\""@en ;
               rdfs:label "Light body wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#LowAcidityWine
:LowAcidityWine rdf:type owl:Class ;
                rdfs:subClassOf :Acidity ;
                rdfs:comment "Wines with acidity lower or equal to 3"@en ;
                rdfs:label "Low acidity wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#MediumBodyWine
:MediumBodyWine rdf:type owl:Class ;
                rdfs:subClassOf :Body ;
                rdfs:comment "Medium body wine is a type of wine that falls in between light and full-bodied wines. It typically has a moderate alcohol content and a balance of flavor and texture, making it a versatile wine that can pair well with a range of foods."@en ;
                rdfs:label "medium body wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#NumberOfReviews
:NumberOfReviews rdf:type owl:Class ;
                 rdfs:subClassOf :Wine ;
                 rdfs:comment "The total number of reviews given to a wine by critics or industry specialists." ;
                 rdfs:label "Number of reviews" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#OverTenThousendReviewsWine
:OverTenThousendReviewsWine rdf:type owl:Class ;
                            rdfs:subClassOf :NumberOfReviews ;
                            owl:disjointWith :UnderFortyReviewsWine ;
                            rdfs:comment "Wines with more than 10000 reviews" ;
                            rdfs:label "Over ten thousand reviews wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pairing
:Pairing rdf:type owl:Class ;
         rdfs:subClassOf :Wine ;
         rdfs:comment "Pairing is the art of combining food and wine to enhance the flavors and create a harmonious dining experience. The right pairing can elevate a dish to a whole new level and make the wine taste even better. It's all about finding the perfect balance between the food and the wine to create a truly memorable meal."@en ;
         rdfs:label "Pairing" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Price
:Price rdf:type owl:Class ;
       rdfs:subClassOf :Wine ;
       rdfs:comment "The wine price represents the cost of a bottle of wine in a particular currency. Wine prices can vary widely depending on factors such as grape variety, winemaking techniques, ageing, and the reputation of the producer or region. Wine prices can also be influenced by external factors such as supply and demand, economic conditions, and exchange rates."@en ;
       rdfs:label "Price" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Pricy
:Pricy rdf:type owl:Class ;
       rdfs:subClassOf :Price ;
       rdfs:comment "Wines with price equals to 60 but less than 300"@en ;
       rdfs:label "Pricy" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Rating
:Rating rdf:type owl:Class ;
        rdfs:subClassOf :Wine ;
        rdfs:comment "This property is used to indicate the quality or rating of a particular wine, usually on a scale from 0 to 5. This rating can be given by various wine critics or publications and is used as a guide to help consumers make informed choices when purchasing wine." ;
        rdfs:label "Rating" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#RedMeat
:RedMeat rdf:type owl:Class ;
         rdfs:subClassOf :Food ;
         owl:disjointWith :SeaFood ;
         rdfs:comment "Red meat is a food category that includes various types of meat, such as beef, lamb, and pork, which are typically red or dark in color when raw. These meats are often considered rich in protein and iron, and are commonly paired with full-bodied red wines to complement their flavors."@en ;
         rdfs:label "Red meat" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Region
:Region rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :regionName ;
                              owl:someValuesFrom xsd:string
                            ] ;
        rdfs:subClassOf :WineEntity ;
        rdfs:comment "A WineRegion represents a specific geographical area known for wine production, characterised by its unique climate, soil, and topography. Each region may have distinct regulations and wine styles, and it can contain multiple subregions and wineries."@en ;
        rdfs:label "Region" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Salmon
:Salmon rdf:type owl:Class ;
        rdfs:subClassOf :Fish ;
        rdfs:comment "Salmon is a type of fish that is native to the North Atlantic and Pacific Oceans and is known for its delicate flavor, rich texture, and versatility in cooking. It is commonly grilled, baked, or smoked, and is a popular ingredient in salads, sushi, and other dishes. Salmon is also a good source of omega-3 fatty acids, protein, and other nutrients."@en ;
        rdfs:label "Salmon" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#SeaFood
:SeaFood rdf:type owl:Class ;
         rdfs:subClassOf :Food ;
         rdfs:comment "Seafood refers to a wide range of edible sea creatures, including fish, shellfish, and other marine animals. These dishes are often enjoyed for their unique flavors and health benefits, such as being low in fat and high in protein. From grilled fish to seafood paella, there are many delicious and nutritious ways to enjoy this culinary category."@en ;
         rdfs:label "Sea food" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#UnderFortyReviewsWine
:UnderFortyReviewsWine rdf:type owl:Class ;
                       rdfs:subClassOf :NumberOfReviews ;
                       rdfs:comment "Wines with less than 40 reviews" ;
                       rdfs:label "Under forty reviews wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#VeryHighRatedWine
:VeryHighRatedWine rdf:type owl:Class ;
                   rdfs:subClassOf :Rating ;
                   rdfs:comment "Medium rated wine refers to a wine that has received a score or rating in the middle of the rating scale, usually indicating a good but not exceptional quality. It may be a versatile choice for pairing with different types of food or enjoyed on its own." ;
                   rdfs:label "Very high rated wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#Wine
:Wine rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :wineName ;
                            owl:someValuesFrom xsd:string
                          ] ;
      rdfs:subClassOf :WineEntity ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasVariety ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :GrapeVariety
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :pairsWith ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Food
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasWineType ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :WineType
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :producedBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Winery
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :producedIn ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Region
                      ] ;
      rdfs:comment "Wine is an alcoholic beverage made from fermented grapes. It can vary greatly in flavor, color, and texture depending on factors such as grape variety, region, and winemaking techniques. Wines are commonly classified into categories such as red, white, rosé, and sparkling based on their color and carbonation level."@en ;
      rdfs:label "Wine" .


###  http://www.semanticweb.org/ontology/baco-spanish-wine-ontology#WineEntity
:WineEntity rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :entityName ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] ;
